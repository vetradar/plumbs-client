{
    "name": "@vetradar/plumbs-client",
    "version": "0.0.6",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "private": false,
    "author": {
        "name": "Vet Radar",
        "email": "team@vetradar.com"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vetradar/plumbs-client.git"
    },
    "license": "MIT",
    "scripts": {
        "dev": "tsup --watch",
        "build": "tsc",
        "bundle": "tsup",
        "prettier": "prettier --write \"src/**/*.ts\"",
        "clean": "rm -rf dist lib"
    },
    "devDependencies": {
        "@types/node": "^20.10.5",
        "prettier": "^3.1.1",
        "tsup": "^8.0.1",
        "typescript": "^5.3.2"
    },
    "tsup": {
        "entry": [
            "src/index.ts"
        ],
        "splitting": false,
        "sourcemap": true,
        "clean": true,
        "dts": true,
        "format": [
            "esm",
            "cjs"
        ],
        "treeshake": true,
        "minify": true
    }
}
