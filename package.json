{
    "name": "@vetradar/plumbs-client",
    "version": "0.0.1",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "private": true,
    "author": {
        "name": "Vet Radar",
        "email": "team@vetradar.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/vetradar/plumbs-client.git"
    },
    "license": "MIT",
    "scripts": {
        "dev": "tsup --watch",
        "build": "tsc",
        "bundle": "tsup",
        "prettier": "prettier --write \"src/**/*.ts\"",
        "clean": "rm -rf dist lib"
    },
    "dependencies": {
        "axios": "^1.5.1"
    },
    "devDependencies": {
        "prettier": "^3.0.3",
        "tsup": "^7.2.0",
        "typescript": "^5.2.2"
    },
    "tsup": {
        "entry": [
            "src/index.ts"
        ],
        "splitting": false,
        "sourcemap": true,
        "clean": true,
        "dts": true,
        "format": [
            "esm",
            "cjs"
        ],
        "treeshake": true,
        "minify": true
    }
}
