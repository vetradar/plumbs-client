{
    "name": "@vetradar/plumbs-client",
    "version": "0.0.1",
    "type": "module",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "private": true,
    "author": {
        "name": "Vet Radar",
        "email": "team@vetradar.com"
    },
    "license": "MIT",
    "scripts": {
        "dev": "tsup --watch",
        "build": "tsc",
        "bundle": "tsup",
        "prettier": "prettier --write \"src/**/*.ts\""
    },
    "dependencies": {
        "axios": "^1.5.1"
    },
    "devDependencies": {
        "prettier": "^3.0.3",
        "tsup": "^7.2.0",
        "typescript": "*"
    },
    "tsup": {
        "entry": [
            "src/index.ts"
        ],
        "splitting": false,
        "sourcemap": true,
        "clean": true,
        "dts": true,
        "format": [
            "esm",
            "cjs"
        ],
        "treeshake": true,
        "minify": true
    }
}
